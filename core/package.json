{
  "name": "@ndjinn/core",
  "version": "0.3.1",
  "description": "Function composition for the ndjinn node editor. Generally useful for geeking out.",
  "main": "./dist/core.es.js",
  "module": "./dist/core.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": "./dist/core.es.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "test": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.32",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3",
    "vite": "^2.6.14"
  },
  "keywords": [
    "function",
    "composition",
    "ndjinn",
    "node",
    "editor"
  ],
  "author": "Austin Martin (auz.martin@gmail.com)",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.spec.(ts|js)"
    ],
    "testEnvironment": "jsdom"
  }
}
