{
  "name": "@ndjinn/core",
  "version": "0.4.0",
  "description": "Function composition for the ndjinn node editor. Generally useful for geeking out.",
  "type": "module",
  "main": "./dist/core.umd.cjs",
  "module": "./dist/core.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "require": "./dist/core.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "tsc": "wireit",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "wireit": {
    "dev": {
      "command": "vite",
      "files": ["src/**/*", "vite.config.ts"]
    },
    "build": {
      "command": "vite build",
      "dependencies": [
        "tsc"
      ],
      "files": ["src/**/*", "vite.config.ts"],
      "output": ["dist/**/*"]
    },
    "tsc": {
      "command": "tsc",
      "files": ["src/**/*.ts", "tsconfig.json"],
      "output": ["dist/types/**/*"]
    }
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vitest": "^0.26.2",
    "wireit": "^0.9.2"
  },
  "keywords": [
    "function",
    "composition",
    "ndjinn",
    "node",
    "editor"
  ],
  "author": "Austin Martin (auz.martin@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "rxjs": "^7.5.2"
  }
}
