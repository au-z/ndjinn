{
  "name": "@ndjinn/ui",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "test": "vitest"
  },
  "wireit": {
    "dev": {
      "command": "vite",
      "dependencies": [
        "../core:build"
      ],
      "files": [
        "src/**/*",
        "vite.config.ts"
      ]
    },
    "build": {
      "command": "vite build",
      "dependencies": [
        "../core:build"
      ],
      "files": [
        "src/**/*",
        "vite.config.ts"
      ],
      "output": [
        "dist"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/three": "^0.139.0",
    "autoprefixer": "^10.4.4",
    "hybrids": "^7.0.5",
    "mousetrap": "^1.6.5",
    "rollup-plugin-css-hmr": "^1.0.0",
    "polyfill-crypto.getrandomvalues": "^1.0.0",
    "postcss": "^8.4.12",
    "postcss-mixins": "^9.0.2",
    "postcss-nested": "^5.0.6",
    "redux": "^4.1.2",
    "tailwindcss": "^3.0.23",
    "vite": "^4.0.3",
    "vitest": "^0.26.2",
    "wireit": "^0.9.2"
  },
  "dependencies": {
    "@auzmartist/cam-el": "^3.0.0-alpha.4",
    "@auzmartist/hybrids-helpers": "^0.3.4",
    "@ndjinn/core": "workspace:*",
    "code-tex": "^1.1.0",
    "immer": "^9.0.12",
    "three": "^0.148.0"
  }
}
